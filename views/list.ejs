<%- include ("header") -%>

    <div class="box" id="heading">
        <h1><%= listTitle %></h1>
    </div>
    <div class="box">
        
            <%for(var i=0;i<newListItem.length;i++){%>
            <div class="item">
            <input type="checkbox">
            <p><%= newListItem[i] %></p><br>
           
        </div>
        <% } %>
        <form  class="item" action="/" method="post">
            <input  name="newItem" type="text" placeholder="Enter the task" autocomplete="off">
            <button type="submit" name="list" value= <%= listTitle %> >+</button>
        </form>
    </div>
   
    <%- include ("footer") -%>